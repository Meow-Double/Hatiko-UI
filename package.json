{
  "name": "hatiko-ui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint:ts": "npx eslint --config .eslintrc.cjs \"src/**/*.{ts,tsx}\"",
    "lint:ts:fix": "npx eslint --config .eslintrc.cjs \"src/**/*.{ts,tsx}\" --fix",
    "lint:css": "stylelint \"**/*.css\"",
    "lint:css:fix": "stylelint \"**/*.css\" --fix",
    "format": "prettier --write \"*.{ts,tsx}\"",
    "format:fix": "prettier --write \"src/**/*.{ts,tsx}\" --fix",
    "preview": "vite preview",
    "prepare": "husky"
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "eslint": "^8.57.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@meow-double/eslint": "^1.0.1",
    "@meow-double/prettier": "^1.0.1",
    "@meow-double/stylelint": "^1.0.1",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@vitejs/plugin-react": "^4.3.4",
    "husky": "^9.1.7",
    "prettier": "^3.5.3",
    "stylelint": "^16.19.1",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.22.0",
    "vite": "^6.1.0"
  }
}
